{
  "redirects": [
    {
      "source": "/",
      "destination": "/statools/",
      "permanent": false
    }
  ],
  "rewrites": [
    {
      "source": "/statools/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/statools/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/statools",
      "destination": "/statools/index.html"
    },
    {
      "source": "/statools/:path*",
      "destination": "/statools/index.html"
    }
  ]
}
